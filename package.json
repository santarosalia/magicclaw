{
  "name": "magicclaw",
  "version": "0.1.0",
  "private": true,
  "description": "AI agent with MCP server management",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "build:electron": "pnpm --filter api build && pnpm --filter web build && pnpm --filter electron build",
    "dev": "pnpm -r --parallel dev",
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "dev:electron": "pnpm --filter electron dev",
    "start:electron": "NODE_ENV=production pnpm --filter electron start",
    "dist:electron": "NODE_ENV=production pnpm --filter electron dist",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  }
}
